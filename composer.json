{
    "name": "aureola/laravel-teapot",
    "description": "Laravel teapot to trap vulnerability scanners and integrate with fail2ban via HTTP 418",
    "keywords": ["laravel", "statamic", "teapot", "fail2ban", "security", "vulnerability-scanner"],
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Hanne",
            "homepage": "https://aureola.codes"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/routing": "^10.0|^11.0|^12.0"
    },
    "suggest": {
        "statamic/cms": "For Statamic CMS integration"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Aureola\\LaravelTeapot\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Aureola\\LaravelTeapot\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aureola\\LaravelTeapot\\TeapotServiceProvider"
            ]
        },
        "statamic": {
            "name": "Statamic Teapot",
            "description": "Statamic teapot to trap vulnerability scanners and integrate with fail2ban via HTTP 418"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
